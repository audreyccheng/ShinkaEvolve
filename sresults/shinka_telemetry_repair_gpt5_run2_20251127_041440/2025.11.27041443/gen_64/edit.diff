         out['interface_status'] = (status_orig, repaired_status, status_conf)
 
         # Copy metadata unchanged
         out['connected_to'] = data.get('connected_to')
         out['local_router'] = local_router
         out['remote_router'] = remote_router
 
         result[if_id] = out
 
     return result
 
