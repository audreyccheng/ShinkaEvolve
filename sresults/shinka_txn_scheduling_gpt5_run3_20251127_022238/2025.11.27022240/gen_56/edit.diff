     def merge_blocks(blocks):
         """Repeatedly merge blocks pairwise until single sequence remains."""
         cur = [list(b) for b in blocks]
         while len(cur) > 1 and time_left():
             nxt = []
             for k in range(0, len(cur), 2):
                 if k + 1 < len(cur):
